endpoints {
  action-endpoints {

    action-endpoint (ShowMenu) {
      accepted-inputs (order)
      local-endpoint (ShowMenu.js)
    }
    action-endpoint (ShowCart) {
      accepted-inputs (order)
      local-endpoint (ShowCart.js)
    }
    action-endpoint (AddItem) {
      accepted-inputs (order, userRequest)
      local-endpoint (AddItem.js)
    }
    action-endpoint (CreateOrder) {
      accepted-inputs ()
      local-endpoint (CreateOrder.js)
    }
  }
}
